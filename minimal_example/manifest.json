{
    "manifest_version": 3,
    "name": "Message passing test",
    "version": "1.0",
    "description": "Logs the current tab's URL to the console",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }